# Backend (MySQL) - use apenas no servidor, nunca no frontend
DB_HOST=
DB_PORT=3306
DB_USER=
DB_PASSWORD=
DB_NAME=

# Servidor API
PORT=3000
# Origens permitidas no CORS (separadas por vírgula). Em produção use a URL do frontend (ex.: https://seu-app.vercel.app).
CORS_ORIGIN=http://localhost:5173

# Frontend (Vite) - prefixo VITE_ expõe ao bundle
# Em produção (ex.: Vercel) use a URL da sua API deployada (Railway, Render, etc.), não localhost.
VITE_API_URL=http://localhost:3000
VITE_SUPABASE_URL=
VITE_SUPABASE_ANON_KEY=
